{
  "manifest_version":2,
  "name":"Browser Screen",
  "version":"1.0",
  "description":"Automatic page refresh on errors",
  "background":{
    "scripts":[
      "background.js"
    ]
  },
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "permissions":[
    "webRequest",
    "tabs",
    "<all_urls>"
  ]
}
